{
  "compilerOptions": {
    "target": "ES2022",
    // "module": "ES2022",
    "module": "CommonJS",
    "lib": [
      "ES2022"
    ],
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "removeComments": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "typeRoots": [
      "./types",
    ],
    "skipLibCheck": true,
  }
}